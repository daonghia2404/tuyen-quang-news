mixin List({ title, data = [] })
  .List
    .List-header 
      .List-title= title
    .List-body 
      each item in data
        a(href=`${item.link}` target=`${item.target || '_self'}`).List-item.flex.items-start 
          span.List-item-icon 
            <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M2.5 15H7.5V13.3333H2.5V15ZM2.5 5V6.66667H17.5V5H2.5ZM2.5 10.8333H12.5V9.16667H2.5V10.8333Z" fill="#6F7789"/></svg>
          span.List-item-title= item.title
